{
  "name": "stock-take-cloudflare-worker",
  "version": "1.0.0",
  "description": "Cloudflare Worker API for Stock Take App",
  "main": "src/index.js",
  "scripts": {
    "build": "node build-static-files.js",
    "dev": "npm run build && wrangler dev",
    "deploy": "npm run build && wrangler deploy",
    "db:create": "wrangler d1 create stock-take-db",
    "db:migrate": "wrangler d1 execute --database-id=7b0985e4-b27d-4c39-9438-0b638ae8b469 --file=./schema.sql",
    "db:query": "wrangler d1 execute stock-take-db --command"
  },
  "keywords": [
    "cloudflare",
    "worker",
    "d1",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "wrangler": "^4.50.0"
  }
}

